<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="https://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://www.woltlab.com https://www.woltlab.com/XSD/vortex/templateListener.xsd">
	<import>
		<templatelistener name="uploadFilterAvatarReplaceCSS">
			<environment>user</environment>
			<templatename>headInclude</templatename>
			<eventname>stylesheets</eventname>
			<templatecode><![CDATA[{if $uploadFilterRemoveAvatar|isset && $uploadFilterRemoveAvatar === true}<style>img.userAvatarImage{ display: none }</style>{/if}]]></templatecode>
		</templatelistener>
		
		<templatelistener name="uploadFilterAvatarReplaceJS">
			<environment>user</environment>
			<templatename>headIncludeJavaScript</templatename>
			<eventname>javascriptInclude</eventname>
			<templatecode><![CDATA[{if $uploadFilterRemoveAvatar|isset && $uploadFilterRemoveAvatar === true}<script data-relocate="true">WCF.DOMNodeInsertedHandler.addCallback('WCF.UploadFilterAvatarReplace', function() { elBySelAll('img.userAvatarImage:not(.blocked)').forEach(function(img) { elAttr(img, 'src', 'data:image/png;base64,'); img.style.display = 'block'; img.classList.add('blocked'); }); }); WCF.DOMNodeInsertedHandler.execute();{/if}</script>]]></templatecode>
		</templatelistener>
	</import>
</data>
